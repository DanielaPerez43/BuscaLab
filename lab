			<script type="text/javascript">
function comprobarHorario(){
         var selectDia = document.getElementById("selectDia").value;
         var mes = document.getElementById("mesCal").value;
         var year = document.getElementById("yearCal").value;
         document.getElementById("campoHorarioFin").innerHTML="";
         $.post('ajax/compruebaHorario.php', {selectDia: selectDia, mes: mes, year: year}, function(dataHorarios){
                    $("#campoHorario").html(dataHorarios);
                });
                
     }	
     function comprobarHorarioFin(){
         var selectDia = document.getElementById("selectDia").value;
         var mes = document.getElementById("mesCal").value;
         var year = document.getElementById("yearCal").value;
         var horaInicio = document.getElementById("selectHoraInicio").value;
         $.post('ajax/compruebaHorarioFin.php', {selectDia: selectDia, mes: mes, horaInicio: horaInicio, year: year}, function(dataHorariosFin){
                    $("#campoHorarioFin").html(dataHorariosFin);
                });
         
     }
     function comprobarReservacion(){
         var output='<input type="submit" name="reservar" value="Reservar">';
         $("#botonReservar").html(output);
     }
</script>
